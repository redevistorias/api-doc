          "express": {
            "type": "boolean",
            "example": true,
            "description": "Opção por vistoria express."
          },
          "details": {
            "type": "string",
            "example": "Retirar as chaves às 14:00.",
            "description": "Informações adicionais importantes ao pedido, por exemplo, local das chaves, horário de retirada, etc."
          }
        }
      },
