            "enum": [
              "furnished",
              "semi-furnished",
              "unfurnished"
            ]
          },
          "modality": {
            "type": "string",
            "example": "premium",
            "description": "Modalidade do pedido",
            "enum": [
              "standard",
              "premium"
            ]
          },
          "express": {
            "type": "boolean",
            "example": true,
            "description": "Opção por vistoria express"
          }
