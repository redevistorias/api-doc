          "client_type",
          "document",
          "name",
          "email",
          "phone",
          "address",
          "hooks"
        ],
        "properties": {
          "client_type": {
            "type": "string",
            "example": "pf",
            "description": "pf para Pessoa Física"
          },
          "document": {
            "type": "string",
            "example": "000.000.999-99",
